{
  "canisters": {
    "godwin_backend": {
      "main": "src/godwin_backend/main.mo",
      "type": "motoko",
      "declarations": {
        "output": "src/godwin_frontend/declarations/godwin_backend"
      }
    },
    "godwin_frontend": {
      "dependencies": [
        "godwin_backend"
      ],
      "frontend": {
        "entrypoint": "index.html"
      },
      "source": [
        "src/godwin_frontend/assets",
        "src/godwin_frontend/dist"
      ],
      "type": "assets",
      "declarations": {
        "output": "src/godwin_frontend/declarations/godwin_frontend"
      }
    },
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm",
      "shrink": false,
      "remote": {
        "candid": "internet_identity.did",
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "declarations": {
        "output": "src/godwin_frontend/declarations/internet_identity"
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "vessel sources"
    },
    "replica": {
      "subnet_type": "system"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    }
  },
  "version": 1
}